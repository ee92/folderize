{
	"name": "folderize",
	"displayName": "folderize",
	"description": "Easily convert a Javascript file into a folder with an index file without breaking imports / exports. Supports .js, .jsx, .ts, .tsx.",
	"version": "0.0.6",
	"publisher": "ee92",
	"repository": "https://github.com/ee92/folderize",
	"icon": "assets/logo.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"keywords": [
		"folder",
		"file",
		"convert",
		"index"
	],
	"activationEvents": [
		"onCommand:extension.folderize"
	],
	"main": "./dist/folderize.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.folderize",
				"title": "Folderize"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.folderize",
					"group": "boilerplate@1"
				}
			],
			"explorer/context": [
				{
					"command": "extension.folderize",
					"group": "boilerplate@1"
				}
			]
		},
		"configuration": {
			"title": "Folderize",
			"properties": {
				"folderize.optionalFilesToGenerate": {
					"type": "array",
					"default": [
						"test",
						"css_module"
					],
					"markdownDescription": "Customize the files you want to generate by selecting from the presets or creating your own templates. To learn more about how to customize this setting go to the [documentation](https://github.com/ee92/folderize)."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack",
		"watch": "webpack --watch",
		"pretest": "npm run compile"
	},
	"devDependencies": {
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"ts-loader": "^6.0.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"webpack": "^4.38.0",
		"webpack-cli": "^3.3.6"
	},
	"license": "SEE LICENSE IN LICENSE.md"
}